package myPracticeSir;

public class OopsMCQ
{
	public static void main(String[] args) 
	{
		int c=0;
		int n=13;
		for (int i=2;i<=n;i++)
		{
			if(n%i==0)
			{
				c++;
			}
		}
			if(c==1) 
			{
				System.out.println("prime...");
			}
			else 
			{
				System.out.println("not a prime...");
			}
	}
}

	
